{
    "name": "altv-quickstart-ts",
    "version": "4.1.0",
    "description": "The Fastest Typescript Repo for alt:V",
    "main": "",
    "scripts": {
        "clean": "rimraf ./resources",
        "build": "npm run clean && swc ./src -d ./resources --copy-files",
        "update": "altv-pkg d release",
        "windows": "npm run build && altv-server.exe",
        "linux": "npm run build && ./start.sh",
        "windev": "nodemon --watch \"./src/**\" --ext \"ts\" --exec \"npm run build && altv-server.exe\"",
        "dev": "npm run windev",
        "reinstall": "rimraf node_modules"
    },
    "author": "stuyk",
    "license": "ISC",
    "devDependencies": {
        "@altv/types-client": "^2.1.1",
        "@altv/types-natives": "^1.4.4",
        "@altv/types-server": "^2.2.8",
        "@altv/types-shared": "^1.2.1",
        "@altv/types-webview": "^1.0.2",
        "@swc/cli": "^0.1.57",
        "@swc/core": "^1.2.186",
        "@types/node": "^14.6.1",
        "altv-pkg": "^2.0.3",
        "nodemon": "^2.0.16",
        "rimraf": "^3.0.2",
        "tsup": "^4.11.2",
        "typescript": "latest"
    },
    "prettier": {
        "tabWidth": 4,
        "semi": true,
        "printWidth": 120,
        "arrowParens": "always",
        "singleQuote": true
    },
    "type": "module"
}
